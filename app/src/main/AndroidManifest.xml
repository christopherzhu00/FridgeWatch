<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hack.ucla.lahacks" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainNavActivity"
            android:label="@string/title_activity_main_nav"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShoppingListActivity"
            android:label="@string/title_activity_shopping_list"
            android:parentActivityName=".MainNavActivity"
            android:screenOrientation="landscape" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="hack.ucla.lahacks.MainNavActivity" />
        </activity>
        <activity
            android:name=".myDialogFrag"
            android:label="@string/title_activity_my_dialog_frag"
            android:parentActivityName=".MainNavActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="hack.ucla.lahacks.MainNavActivity" />
        </activity>
    </application>

</manifest>
